<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - TracNghiemOnline</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/TracNghiemOnline.styles.css" asp-append-version="true" />
  
 
</head>
<body>
 
    
            <div class="container">
                <div class="row ">
                    <div class="col-md-2 bg-black ">
                        <div class="container  text-white ">

                            <h4 class="text-center">Admin</h4>




                            <h5>Quản lí</h5>

                        <p> <a asp-action="Index" asp-controller="Dethis" asp-area="Admin">Đề thi</a></p>

                   

                        </div>
                    </div>
                    <div class="col-md-10 ">
                        <div class="container bg-black  ">
                            <h1 class="text-white">Dashboards</h1>
                        </div>
                    <div class="container">
                        <main role="main" class="pb-3">
                            @RenderBody()
                        </main>
                    </div>

                    </div>
                </div>
            </div>

    
  

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - TracNghiemOnline - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/Test.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
