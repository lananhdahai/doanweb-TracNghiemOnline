@model IEnumerable<TracNghiemOnline.Models.Mucdokho>

<h2>Mucdokho List</h2>

<p>
    <a asp-action="Add" class="btn btn-primary">Add New</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>Tendk</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Tendk</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Iddokho">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Iddokho">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Iddokho">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
